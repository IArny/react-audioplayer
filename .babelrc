{
  "presets": [
    "react",
    ["babel-preset-latest", {"modules": false}]
  ],
  "plugins": [
    "transform-object-assign",
    ["transform-class-properties", { "spec": true }],
    "react-hot-loader/babel"
  ],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
